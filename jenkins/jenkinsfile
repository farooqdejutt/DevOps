pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'here we will clone the code from github'
            }
        }
                stage('build') {
            steps {
                echo 'here we will build the docker image'
            }
        }
                stage('push') {
            steps {
                echo 'here we will push the build to dockerhub'
            }
        }
                        stage('QA') {
            steps {
                echo 'here will QA the docker image'
            }
        }
                        stage('Day today') {
            steps {
                echo 'today is monday'
            }
        }
                        stage('test') {
            steps {
                echo 'here we will test the build to dockerhub'
            }
        }
    }
}
