pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'here we will clone the code from github'
            }
        }
                stage('build') {
            steps {
                echo 'here we will build the docker image'
            }
        }
                stage('push') {
            steps {
                echo 'here we will push the build to dockerhub'
            }
        }
    }
}